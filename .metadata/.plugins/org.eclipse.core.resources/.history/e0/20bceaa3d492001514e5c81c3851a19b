package libro2;
import java.util.Scanner;
public class LeerLibroCalificaciones {

	public static void main(String[] args) 
	{
		System.out.println("ingrese la cantidad de estudiantes del curso");
		Scanner entrada=new Scanner(System.in);
		int numEstudiantes=entrada.nextInt();
		int notas[]=new int[numEstudiantes];
		int contador=0;
		while(contador<notas.length)
		{
			int nota=(int) Math.floor(Math.random()*100);
			System.out.println(nota);
			contador++;
		}
		System.out.println(contador);		
	}

}
