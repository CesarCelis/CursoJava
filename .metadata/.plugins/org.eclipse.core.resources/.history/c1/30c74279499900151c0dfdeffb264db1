package libro;

import javax.swing.JOptionPane;

public class SumaDeEnteros {

	public static void main(String[] args) 
	{	
		Numeros numero1=new Numeros();
		Numeros numero2=new Numeros();
		int num1=Integer.parseInt(JOptionPane.showInputDialog("Ingrese Número 1: "));
		numero1.setNumero(num1);
		int num2=Integer.parseInt(JOptionPane.showInputDialog("Ingrese Número 2: "));
		numero2.setNumero(num2);	
		int suma=numero1.sumarNumeros(numero1.getNumero(), numero2.getNumero());	
		System.out.println("La suma de "+numero1.getNumero()+" + "+numero2.getNumero()+"= "+suma);
		int suma2=numero1.sumarNumeros(num1, num2);
		System.out.println(suma2);

	}

}
